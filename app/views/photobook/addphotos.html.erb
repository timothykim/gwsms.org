	<input type="button" value="Upload Photos..." onclick="swfu.selectFiles()" style="font-size: 8pt;" /> <input id="btnCancel" type="button" value="Cancel All Uploads" onclick="swfu.cancelQueue();" disabled="disabled" style="font-size: 8pt;" />

<% end -%>


<fieldset id="fsUploadProgress">
	<legend>Uploaded Photos</legend>
	
</fieldset>
<div id="divStatus">0 Files Uploaded</div>

